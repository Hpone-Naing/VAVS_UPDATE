    <script type="text/javascript" src="~/lib/jquery/dist/jquery.min.js" asp-append-version="true"></script>
    <script type="text/javascript" src="~/lib/jquery-ui/1.12.1/jquery-ui.min.js" asp-append-version="true"></script>
