    <link rel="stylesheet" href="~/LoginFormStyle/page-auth.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/style.css" asp-append-version="true" />
    <script src="~/LoginFormStyle/js/helpers.js" asp-append-version="true"></script>
    <script src="~/LoginFormStyle/js/config.js" asp-append-version="true"></script>

